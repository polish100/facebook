<div class="container">
  <div class="wrapper col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1">
    <% @users.each do |user| %>
    <div class="row">
      <%= profile_img(user) if profile_img(user) %>
      <h4><strong><%= user.name %></strong></h4>
      <p><%= user.email %></p>
      <%= render 'follow_form', user: user %>
    </div>
    <% end %>
  </div>
</div>
